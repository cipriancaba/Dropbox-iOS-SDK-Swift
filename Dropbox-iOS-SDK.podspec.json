{
  "name": "Dropbox-iOS-SDK",
  "version": "1.3.14",
  "summary": "The Dropbox SDK for iOS.",
  "homepage": "https://www.dropbox.com/developers/core/sdks/osx",
  "authors": "Dropbox",
  "source": {
    "http": "https://www.dropbox.com/static/developers/dropbox-ios-sdk-1.3.13.zip"
  },
  "platforms": {
    "ios": null
  },
  "source_files": "dropbox-ios-sdk-1.3.13/DropboxSDK.framework/Headers/*.h",
  "exclude_files": "dropbox-ios-sdk-1.3.13/DropboxSDK.framework/Headers/MPOAuth.h",
  "preserve_paths": ["dropbox-ios-sdk-1.3.13/DropboxSDK.framework", "module.map"],
  "license": {
    "type": "Copyright",
    "file": "dropbox-ios-sdk-1.3.13/LICENSE"
  },
  "frameworks": [
    "Security",
    "QuartzCore",
    "DropboxSDK"
  ],
  "prepare_command": '<<-CMD
    cat > "module.map" << MAP
module DropboxSDK [system] {
    header "dropbox-ios-sdk-1.3.13/DropboxSDK.framework/Headers/DropboxSDK.h"
    link "DropboxSDK"
    export *
}
MAP
  CMD',
  "module_map": "module.map",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/Dropbox-iOS-SDK/dropbox-ios-sdk-1.3.13\""
  },
  "requires_arc": false
}
